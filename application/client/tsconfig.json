{
    "compilerOptions": {
	"target": "es5",
	"lib": [
	    "dom",
	    "dom.iterable",
	    "esnext"
	],
	"skipLibCheck": true,
	"esModuleInterop": true,
	"strict": true,
	"forceConsistentCasingInFileNames": true,
	"module": "esnext",
	"moduleResolution": "node",
	"resolveJsonModule": true,
	"isolatedModules": true,
	"noEmit": true,
	"jsx": "react",
	"allowSyntheticDefaultImports": true,
	"allowJs": true,
	"noImplicitAny": true,
	"strictNullChecks": true,
	"strictFunctionTypes": true
    },
    "include": [
	"src"
    ]
}
